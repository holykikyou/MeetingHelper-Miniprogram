.page {
  background:  #fff;
}
.searchLetter {
  position: fixed;
  right: 0;
  width: 73rpx;
  text-align: center;
  justify-content: center;
  display: flex;
  flex-direction: column;
  color: #666;
  z-index: 1;
}

.searchLetter view {
  margin-top: 20rpx;
}

.touchClass {
  color: #fff;
  margin-top: 173px;
  background: #fff;
}

.showSlectedLetter {
  background-color: rgba(0, 0, 0, 0.5);
  color: #efefef;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 50%;
  left: 50%;
  margin: -100rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx;
  font-size: 52rpx;
  z-index: 1;
}

.selection {
  display: flex;
  width: 90%;
  flex-direction: column;
}

.item_letter {
  display: flex;
  background-color: #f5f5f5;
  height: 40rpx;
  padding: 16rpx 10rpx;
  align-items: center;
  font-size: 30rpx;
  color: #666;
}

.item_city {
  display: flex;
  background-color:  #fff;
  height: 100rpx;
  padding: 10rpx 34rpx;
  align-items: center;
  border-bottom: 1rpx solid #ededed;
  font-size: 34rpx;
  color: #666;
}

.hotcity-common {
  font-size: 24rpx;
  color: #666;
  padding: 0 0 0 30rpx;
}

.thisCity {
  padding-top: 30rpx;
}

.thisCityName {
  display: inline-block;
  border: 1rpx solid #2ab4ff;
  border-radius: 8rpx;
  padding: 10rpx 0;
  font-size: 24rpx;
  color: #2ab4ff;
  text-align: center;
  min-width: 149.5rpx;
  margin: 20rpx 0 20rpx 30rpx;
}

.thishotText {
  color:#2ab4ff;
  font-size: 24rpx;
  margin:0 0 0 -30rpx !important;
  width:90rpx;
}

.slectCity {
  border-color: #2ab4ff !important;
}

.slectCity view {
  color: #2ab4ff !important;
}

.weui-grid {
  position: relative;
  float: left;
  padding: 10rpx 0;
  width: 149.5rpx;
  box-sizing: border-box;
  border: 1rpx solid #ececec;
  border-radius: 8rpx;
  margin: 10rpx 12rpx;
}

.weui-grid__label {
  display: block;
  text-align: center;
  color: #333;
  font-size: 24rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.s-check {
  position: absolute;
  right: 90rpx;
  width: 50rpx;
  height: 50rpx;
}
.s-check image {
  width: 50rpx;
  height: 50rpx;
}
.round-click{
    height: 120rpx;
    width: 120rpx;
    background-color: #0BB20C;
    border-radius: 100%;
    position: fixed;
    bottom: 70rpx;
    right: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 9;
}
.round-click navigator{
    font-size: 32rpx;
    max-width: 80rpx;
    color: #fff;
    text-align: center;
}

.p-container {
	width: 100%;
}
.title {
	width: 100%;
	background: #1D8CE0;
	color: #fff;
	font-size: 36rpx;
	padding: 10rpx 0;
	text-indent: 36rpx;
}
.list-wrap {
	width: 100%;
}
.main-list {
	display: flex;
	width: 100%;
	background:  #fff;
	padding: 16rpx 0;
	border-bottom: 3rpx solid #ddd;
  margin-top: 3rpx;
}
.list-left {
	flex: 1;
	text-align: center;
	padding: 10rpx 0;
}
.list-right {
	flex: 1;
	border-left: 4rpx solid #abe2f9;
	padding: 0 40rpx;
}
.start-time, .end-time {
	height: 32rpx;
 
	line-height: 32rpx;
}
.mark{
  height: 10rpx;
  line-height: 10rpx
}
.list-icon {
	width: 38rpx;
	height: 38rpx;
	border-radius: 50%;
	display: inline-block;
	vertical-align: sub;
}
.list-initiator {
	height: 60rpx;
	line-height: 60rpx;
}
.list-title {
	height: 70rpx;
	line-height: 88rpx;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	width: 400rpx;
	font-size: 38rpx;
}
.list-people {
	display: inline-block;
	color: #aeaeae;
	font-size: 32rpx;
}
.no-data {
	width: 400rpx;
	height: 290rpx;
	margin-top: 270rpx;
}
.image{
    width: 50%;
    height: 50%;
    display: flex;
    align-items: center;
}