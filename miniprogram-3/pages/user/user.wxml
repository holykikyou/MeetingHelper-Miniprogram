<view class="page">
	<view class="user">
    <view>
      <image class="user-head" src="../../images/4.jpg"></image>
    </view>
		<view class="user-wrap">
			<view class="item">
				<view class="item-title">当前用户</view>
				<view class="item-content">{{account}}</view>
			</view>
      <view class="item">
				<view class="item-title">用户ID</view>
				<view class="item-content">{{user_id}}</view>
			</view>
			<view class="item">
				<view class="item-title">是否为管理员</view>
				<view class="item-content">{{isAdmin}}</view>
			</view>
			<view class="item">
				<view class="item-title">绑定管理员</view>
				<view class="item-content">{{admin}}</view>
			</view>
			<view class="item">
				<view class="item-title">联系方式</view>
				<view class="item-content">17801064146</view>
			</view>
		</view>
		<view class="register-wrap">
      <button class="jumper" bindtap="loginOut">切换账号</button>
			<button class="warn" class="register" bindtap="loginOut">退出登录</button>
		</view>
	</view>
</view>
